            '--primary': '#00796B', '--secondary': '#FF8F00', '--success': '#388E3C', '--danger': '#D32F2F',
            '--warning': '#FBC02D', '--info': '#0288D1', '--violet-accent': '#7B1FA2',
            '--bg-main': '#E0F7FA', '--bg-card': '#FFFFFF', '--bg-header-footer': '#B2EBF2',
            '--bg-interactive': '#C8E6C9', '--bg-hover': '#A5D6A7',
            '--bg-admin-section-mobile': '#DDF0F0', 
            '--bg-modal-overlay': 'rgba(0,0,0,0.7)',
